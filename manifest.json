{
    "manifest_version": 3,
    "name": "ChatGPT Hotkeys",
    "description": "Hotkeys for ChatGPT (Hotkeys are shown in console)",
    "version": "2.2",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*"
            ],
            "js": [
                "hotkeys.js"
            ],
            "run_at": "document_end"
        }
    ]
}
